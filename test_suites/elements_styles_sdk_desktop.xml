<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="ElementsSDK Test Suite">
    <groups>
        <run>
            <include name="desktop-regression"/>
        </run>
    </groups>
    <test name="ElementsSDK: Styles(CSS) Tests" parallel="classes" thread-count="10">
        <classes>
            <class name="elementsSDKTests.stylesTests.InputsTest"/>
            <class name="elementsSDKTests.stylesTests.TypographyTest"/>
            <class name="elementsSDKTests.stylesTests.ButtonsTest"/>
            <class name="elementsSDKTests.stylesTests.IconsTest"/>
            <class name="elementsSDKTests.stylesTests.FormsTest"/>
            <class name="elementsSDKTests.stylesTests.TablesTest"/>
            <class name="elementsSDKTests.stylesTests.GridTest"/>
            <class name="elementsSDKTests.stylesTests.PresentationStrategyTest"/>
            <class name="elementsSDKTests.stylesTests.TemplatesTest"/>
            <class name="elementsSDKTests.stylesTests.ResponsiveUtilitiesTest"/>
        </classes>
    </test>
</suite>